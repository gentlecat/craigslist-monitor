{
  "name": "beaten-games",
  "version": "1.0.0",
  "repository": "git@github.com:gentlecat/beaten-games.git",
  "private": true,
  "main": "index.js",
  "scripts": {
    "bundle": "webpack",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "lint": "tslint --project . -c tslint.json 'src/**/*.{ts,tsx}'",
    "prettier": "prettier --write \"./**/*.{tsx,ts,js,jsx,css,json}\""
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@emotion/babel-preset-css-prop": "^10.0.6",
    "@emotion/core": "^10.0.6",
    "@types/react": "^16.7.21",
    "@types/react-dom": "^16.0.11",
    "axios": "^0.18.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-emotion": "^10.0.6",
    "emotion-normalize": "^10.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-select": "^2.3.0",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0"
  },
  "devDependencies": {
    "prettier": "^1.16.1",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "concurrently": "^4.1.0",
    "webpack-cli": "^3.2.1"
  }
}
